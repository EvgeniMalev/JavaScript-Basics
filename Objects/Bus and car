<h2>JavaScript Objects</h2>

<p id="demo"></p>
<p id="demo1"></p>

<script>

const car = {type:"Fiat", model:"600 Abarth", color:"white"};
const autobus = {type:"Ikarus", model:"280", color:"red", seats:"44"};

document.getElementById("demo").innerHTML = "The car type is " + car.type + " " + car.model;
document.getElementById("demo1").innerHTML = "The bus ВТ 8070 АТ is " + autobus.type + " " + autobus.model  +  " with " + autobus.seats +  " seats.";
</script>

